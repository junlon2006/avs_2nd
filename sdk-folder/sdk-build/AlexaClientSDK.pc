Name: AlexaClientSDK
Description: A cross-platform, modular SDK for interacting with the Alexa Voice Service
Version: 1.9.0
Requires: libcurl sqlite3 gstreamer-1.0 gstreamer-app-1.0
prefix=/usr/local
includedir=${prefix}/include
libdir=${prefix}/lib
Libs: -L${libdir} -lAVSCommon -lACL -lADSL -lAFML -lCertifiedSender -lContextManager -lCapabilitiesDelegate -lPlaylistParser -lKWD -lAIP -lAlerts -lAudioPlayer -lBluetooth -lExternalMediaPlayer -lInteractionModel -lMRM -lNotifications -lPlaybackController -lSpeakerManager -lSpeechSynthesizer -lSettings -lAVSSystem -lTemplateRuntime -lDefaultClient -lAudioResources -lMediaPlayer -lESP -lRegistrationManager -lCBLAuthDelegate -lSQLiteStorage
Cflags: -I${includedir}
